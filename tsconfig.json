{
  "extends": "astro/tsconfigs/base",
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "moduleResolution": "node",
    "strict": true,
    "jsx": "react-jsx",
    "allowJs": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "isolatedModules": true,
    "types": ["astro/client"]
  },
  "include": ["src/**/*"],
  "exclude": ["node_modules","**/test-sentry.astro"]
}